<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>


<BorderPane xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.Controllers.AskDoubleCannonController">

    <center>
        <StackPane>
            <ImageView fx:id="backgroundImage" fitHeight="679.0" fitWidth="937.0" pickOnBounds="true" />

            <GridPane fx:id="grid" alignment="CENTER" gridLinesVisible="true" hgap="2" vgap="2">
                <padding>
                    <Insets bottom="10" left="10" right="10" top="10" />
                </padding>
            </GridPane>
        </StackPane>
    </center>

    <right>
        <AnchorPane prefHeight="233.0" prefWidth="404.0">
            <children>
                <Label fx:id="batteriesAndPowerUpdateLabel" layoutY="247.0" prefHeight="91.0" prefWidth="404.0" />
                <Label fx:id="infoLabel" layoutY="335.0" prefHeight="55.0" prefWidth="407.0" />
            <Button fx:id="finishButton" layoutX="173.0" layoutY="430.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="71.0" text="FINISH" />
                <Button fx:id="okButton" layoutX="186.0" layoutY="473.0" mnemonicParsing="false" prefHeight="29.0" prefWidth="47.0" text="OK" />
            </children>
        </AnchorPane>
    </right>
</BorderPane>
